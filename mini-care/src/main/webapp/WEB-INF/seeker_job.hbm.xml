<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="SeekerJob" table="SEEKER_JOB">
		<id name="id" column="ID" />
		<property name="userid" column="userID" />
		<property name="title" column="title" />
		<property name="descr" column="descr" />
		<many-to-one name="users" class="Users" fetch="select">
			<column name="userid" not-null="true" />
		</many-to-one>
		<set name="SITTER" table="Sitter" inverse="false" lazy="true"
			fetch="select" cascade="all">
			<key column="id" />
			<many-to-many column="id" class="Sitter" />
		</set>
	</class>
</hibernate-mapping>